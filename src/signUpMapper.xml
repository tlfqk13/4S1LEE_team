<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="hotel.dao.signUpMapper">

<insert id="signUp" parameterType="HomeUser" >
insert into UserPro (
	userID
	,userPassword
	,userEmail
	,userPhone
	,userName
	,userBirth
)
values (
	#{userID}
	,#{userPassword}
	,#{userEmail}
	,#{userPhone}
	,#{userName}
	,#{userBirth}
)
</insert>

<select id="idDoubleCheck" parameterType="HomeUser" resultType="HomeUser">
select * from UserPro
where
	userID=#{userID}
</select>

<select id="UserTable" parameterType="HomeUser" resultType="HomeUser">
select * from UserPro
</select>


</mapper>
